pipeline{
  agent any
  stages {
    stage('Compile stage') {
      steps {
        echo "Building the project"
        }
      }
    }
    stage('Testing stage'){
      steps {
        echo "Testing the project"
        }
      }
    }
    stage('Deploy stage'){
      steps {
        echo "Deploying the project"
        }
      }
    }
        
